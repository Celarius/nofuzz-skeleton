{
  "name": "nofuzz/skeleton-app",
  "description": "Nofuzz Skeleton App",
  "keywords": [
    "Nofuzz",
    "Skeleton"
  ],
  "homepage": "http://nofuzz.celarius.com",
  "type": "Application",
  "license": "MIT",
  "authors": [
    {
      "name": "Kim Sandell",
      "email": "sandell@celarius.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-PDO": "*",
    "ext-mbstring": "*",
    "ext-Zend-OPCache": "*",
    "nofuzz/framework": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}